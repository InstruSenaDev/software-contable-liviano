---
const { name, encargado, nit, tipeDocument, email, telefono, address } = Astro.props;
---

<tr class="border-b ">
  <td class="p-2">{name}</td>
  <td class="p-2">{encargado}</td>
  <td class="p-2">{nit}</td>
  <td class="p-2">{tipeDocument}</td>
  <td class="p-2">{telefono}</td>
  <td class="p-2">{email}</td>
  <td class="p-2">{address}</td>
</tr>
